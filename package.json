{
  "name": "repolinter-action",
  "version": "0.3.0",
  "private": true,
  "description": "GitHub action to automatically enforce open source policies using Repolinter",
  "main": "lib/entry.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts __tests__/**/*.ts --fix",
    "package": "rm -rf dist && ncc build --source-map -m",
    "test": "npm run build && npm run package && jest",
    "all": "npm run format && npm run lint && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/newrelic/repolinter-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Noah Koontz (nkoontz@newrelic.com)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.4",
    "@octokit/action": "^3.0.5",
    "@octokit/plugin-retry": "^3.0.3",
    "@octokit/rest": "^18.0.3",
    "@octokit/types": "^5.4.0",
    "@types/node-fetch": "^2.5.7",
    "nock": "^13.0.3",
    "node-fetch": "^2.6.0",
    "repolinter": "github:prototypicalpro/repolinter#master"
  },
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/parser": "^3.8.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.6.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^23.20.0",
    "jest": "^26.2.2",
    "jest-circus": "^26.2.2",
    "js-yaml": "^3.14.0",
    "prettier": "2.0.5",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  }
}
